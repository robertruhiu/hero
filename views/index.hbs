        <h3 class="h3">Places of interest</h3>

        <main>
            <!--Main layout-->
            <div class="container">
                {{# each ofinterest}}
                    <!--First row-->
                    <div class="row mt-5 wow">
                        <!--First column-->
                        {{# each this}}
                            <div class="col-lg-4 wow fadeIn" data-wow-delay="0.2s">

                                <!--Card-->
                                <div class="card mb-r">

                                    <!--Card image-->
                                    <img class="img-fluid" src="{{ this.imagePath}}" alt="Card image cap">

                                    <!--Card content-->
                                    <div class="card-body">
                                        <!--Title-->
                                        <h4 class="card-title dark-grey-text"><strong>{{ this.title}}</strong></h4>
                                        <!--Text-->

                                        <p class="card-text">{{ this.description}}</p>
                                        <a class="text">${{ this.price}}</a>
                                        <a href="/add-to-cart/{{this._id}}" class="btn btn-primary btn-md pull-right">Book</a>
                                    </div>

                                </div>
                                <!--/.Card-->

                            </div>
                            <!--/.First column-->
                        {{/each}}
                    </div>
                    <!--/.First row-->
                {{/each}}
            </div>


        </main>

